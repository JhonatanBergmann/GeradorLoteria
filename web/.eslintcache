, array) {\r\n        const aleatorio = parseInt(Math.random() * (max + 1 - min)) + min\r\n        return array.includes(aleatorio)\r\n            ? gerarNumeroNaoContido(min, max, array)\r\n            : aleatorio\r\n    }\r\n\r\n    function gerarNumeros(qtde) {\r\n        const numeros = Array(qtde)\r\n            .fill(0)\r\n            .reduce((nums) => {\r\n                const novoNumero = gerarNumeroNaoContido(1, 60, nums)\r\n                return [...nums, novoNumero]\r\n            }, [])\r\n            .sort((n1, n2) => n1 - n2)\r\n\r\n        return numeros\r\n    }\r\n\r\n    const [qtde, setQtde] = useState(props.qtde || 6)\r\n    const numerosIniciais = gerarNumeros(qtde)\r\n    const [numeros, setNumeros] = useState(numerosIniciais)\r\n\r\n    return (\r\n        <div className=\"Generator\">\r\n            <h2>Aqui estão seus números da sorte!</h2>\r\n            <h3>{numeros.join(\" \")}</h3>\r\n            <div>\r\n                <label>Quantidade de Números</label>\r\n                <input\r\n                    min=\"6\"\r\n                    max=\"15\"\r\n                    type=\"number\"\r\n                    value={qtde}\r\n                    onChange={(e) => {\r\n                        setQtde(+e.target.value)\r\n                        setNumeros(gerarNumeros(+e.target.value))\r\n                    }}\r\n                />\r\n            </div>\r\n            <button onClick={(_) => setNumeros(gerarNumeros(qtde))}>\r\n                Gerar Números\r\n            </button>\r\n        </div>\r\n    )\r\n}",{"ruleId":"21","replacedBy":"22"},{"ruleId":"23","replacedBy":"24"},{"ruleId":"25","severity":1,"message":"26","line":9,"column":1,"nodeType":"27","endLine":29,"endColumn":9},{"ruleId":"25","severity":1,"message":"26","line":4,"column":1,"nodeType":"27","endLine":50,"endColumn":2},"no-native-reassign",["28"],"no-negated-in-lhs",["29"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as mo